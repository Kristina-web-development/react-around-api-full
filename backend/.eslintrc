{
    "extends": "airbnb-base",
    "rules": {
      "array-callback-return": "off",
      "indent": "off",
      "no-console": "off",
      "no-underscore-dangle": "off",
      "max-classes-per-file": 0,
      "no-param-reassign":0,
      "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
    }
  }